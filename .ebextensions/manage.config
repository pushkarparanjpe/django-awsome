packages: 
  yum:
    libpq-dev: []

container_commands:
  collectstatic:
    command: "django-admin.py collectstatic --no-input"
  00migrate:
    command: "django-admin.py migrate"
    leader_only: true
  01createsu:
    command: "django-admin.py createsu"
    leader_only: true

option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "one.settings"

